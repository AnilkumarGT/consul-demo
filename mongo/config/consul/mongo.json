{
    "service": {
        "name": "mongo",
        "port": 27017,
        "tags": [
            "nosql",
            "database"
        ],
        "check": {
            "id": "mongo_status",
            "name": "Mongo Status",
            "args": ["mongo", "--eval", "'printjson(rs.status())'"],
            "interval": "30s",
            "timeout": "20s"
        }
    }
}
